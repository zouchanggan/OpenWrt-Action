config wifi-iface 'wifinet0'
    option device 'radio0'
    option mode 'ap'
    option ssid 'ZeroWrt_5G'
    option encryption 'psk2'
    option key '123456789'
    option network 'lan'

config wifi-iface 'wifinet1'
    option device 'radio1'
    option mode 'ap'
    option ssid 'ZeroWrt_2.4G'
    option encryption 'psk2'
    option key '123456789'
    option network 'lan'
